import SwiftUI

struct ContentView: View {
    var body: some View {

        NavigationView {
            VStack {
                CircleImageView()
               
                HStack {
                    NavigationLink (destination: MyWebview(urlToLoad: "https://www.youtube.com/channel/UCeL_yJ9v2aMj--Vh8JtB_Tg")
                    ) {
                        Text ("구독하러 가기")
                            .font(.system(size: 20))
                            .fontWeight(.bold)
                            .foregroundColor(Color.white)
                            .padding(20)
                            .background(Color.orange)
                            .cornerRadius(20)
                            .padding(10)
                    }
                }
            }
        }
    }
}

