import SwiftUI


struct CircleImageView: View {
    var body: some View {
        Image("juyaaaaa")
            .resizable()
            .scaledToFill()
            .frame(width: 250, height: 250)
            .clipShape(Circle())
            .overlay(Circle().stroke(Color.yellow, lineWidth: 5))
    }
}


struct CircleImageView_Previews: PreviewProvider {
    static var previews: some View {
        CircleImageView()
    }
}
